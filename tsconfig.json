{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": ["vite-plugin-svgr/client"],
    "typeRoots": ["node_modules/@types", "src/@types"]
  },
  "include": [
    "src/**/*",
    "./custom.d.ts", // Include your custom type declaration file
    "./src/vite-env.d.ts" // Include vite-env.d.ts if it provides additional global declarations
  ],
  
}
